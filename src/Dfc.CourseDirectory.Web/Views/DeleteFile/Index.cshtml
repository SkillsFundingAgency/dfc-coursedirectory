@using Dfc.CourseDirectory.Models.Enums
@using Dfc.CourseDirectory.Web.Helpers
@{
    Layout = "_Layout_Your_Courses";
}

@model Dfc.CourseDirectory.Web.ViewModels.PublishCourses.DeleteFileViewModel


<div class="content-container">
    <div class="govuk-grid-row" data-sf-element="Row">
        <div class="govuk-grid-column-two-thirds">

            <form class="govuk-!-margin-bottom-9" id="DownloadErrorFileForm" asp-controller="PublishCourses" asp-action="DeleteFile" method="post">
                @*<a asp-controller="PublishCourses" asp-action="WhatDoYouWantToDoNext" class="govuk-back-link">Back</a><br />*@
                <span class="govuk-caption-xl">Course Directory</span>
                <h1 class="govuk-heading-xl">Delete this file (Stubbed)</h1>

                <p class="govuk-body govuk-!-padding-bottom-4">
                    The courses in this file will not be published to the Course directory.<br />You can upload another file at any time.
                </p>

                <ul class="flex-container">
                    <li class="flex-item">
                        <button type="submit" class="govuk-button" id="next">Continue</button>
                    </li>
                    <li class="flex-item" style="padding-top: 15px">
                        <a class="govuk-link" asp-controller="PublishCourses" asp-action="WhatDoYouWantToDoNext">Cancel</a>
                    </li>
                </ul>
             
            </form>
        </div>
    </div>
</div>

