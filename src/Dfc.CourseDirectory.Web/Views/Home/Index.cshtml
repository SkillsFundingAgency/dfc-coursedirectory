@using Dfc.CourseDirectory.Web.ViewComponents.VenueSearchResult
<h1 class="govuk-heading-xl">Provider Search</h1>

<div class="govuk-grid-row">
    <div class="govuk-grid-column-full">
        <div class="govuk-form-group">
           
            <label class="govuk-label" for="name">
                UK PRN - Holding Page for Provider integration
            </label>

            <input class="govuk-input" id="UKPrn" name="UKPrn" type="text">

            <button type="submit" class="govuk-button" id="search">
                Search
            </button>
        </div>
    </div>
    
    <div id="VenueSearchResultContainer">
        @await Component.InvokeAsync(nameof(VenueSearchResult))
    </div>
</div>